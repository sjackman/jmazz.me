{
  "name": "jmazz.me",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ssr": "nodemon --legacy-watch -w src src/server.js",
    "start": "NODE_ENV=production node src/server.js",
    "static": "node builder/static-builder.js",
    "bundle": "npm run bundle-server && npm run bundle-client",
    "bundle:prod": "NODE_ENV=production npm run bundle",
    "bundle-server": "webpack --config webpack/webpack.node.js",
    "bundle-client": "webpack --config webpack/webpack.client.js",
    "caddy:build": "docker build -t jmazz-caddy . -f ./caddy/Dockerfile",
    "caddy:run": "docker run --rm -p 8080:8080 jmazz-caddy",
    "container:dev": "docker build -t thejmazz/jmazz.me .",
    "container:prod": "docker build -t thejmazz/jmazz.me --build-arg SSR_HOST=https://jmazz.me ."
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "cors": "^2.8.1",
    "dotenv": "^2.0.0",
    "end-of-stream": "^1.1.0",
    "envalid": "^2.4.0",
    "es6-promise": "^3.2.1",
    "express": "^4.14.0",
    "flexboxgrid": "^6.3.1",
    "fs-promise": "^0.5.0",
    "globby": "^6.0.0",
    "highlight.js": "^9.5.0",
    "isomorphic-fetch": "^2.2.1",
    "js-yaml": "^3.6.1",
    "katex": "^0.6.0",
    "lodash": "^4.16.4",
    "marked": "^0.3.5",
    "moment": "^2.15.1",
    "parse-katex": "^0.3.0",
    "split2": "^2.1.0",
    "through2": "^2.0.1",
    "vue": "^2.0.0-rc.5",
    "vue-router": "^2.0.0-rc.4",
    "vue-server-renderer": "^2.0.0-rc.5",
    "vuex": "^2.0.0-rc.5",
    "vuex-router-sync": "^3.0.0"
  },
  "devDependencies": {
    "async-to-gen": "^1.0.5",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-runtime": "^6.9.2",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "file-loader": "^0.9.0",
    "memory-fs": "^0.3.0",
    "node-sass": "^3.9.3",
    "nodemon": "^1.10.2",
    "sass-loader": "^4.0.2",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^2.0.5",
    "vue-html-loader": "^1.2.3",
    "vue-loader": "^9.7.0",
    "vue-style-loader": "^1.0.0",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-middleware": "^1.8.2",
    "webpack-hot-middleware": "^2.12.2"
  }
}
